<section>
    <div class="container-fluid p-5">
        <table class="table">
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">Picture</th>
                <th scope="col">Product</th>
                <th scope="col">Category</th>
                <th scope="col">Brand</th>
                <th scope="col">Price</th>
                <th scope="col">Stock</th>
              </tr>
            </thead>
            <tbody>
            <% let i = 1; %>
            <% products.forEach(product=>{ %>
              <tr>
                <th scope="row"><%= i %></th>
                <td><img style="width: 50px; height: 50px;" src="<%= product.imageAddress %>" alt=""></td>
                <td><%= product.name %></td>
                <td>Smartphones</td>
                <td><%= product.brand %></td>
                <td><%= product.price %></td>
                <td>1000</td>
              </tr>
            <% i++; %>
            <% }) %>
            </tbody>
          </table>
    </div>
</section>